    <TargetFramework>netcoreapp2.2</TargetFramework>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\TimesNewRoman\times.ttf" />
    <None Remove="Fonts\TimesNewRoman\timesbd.ttf" />
    <None Remove="Fonts\TimesNewRoman\timesbi.ttf" />
    <None Remove="Fonts\TimesNewRoman\timesi.ttf" />
    <None Remove="Images\blue-rabbit.pdf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Fonts\TimesNewRoman\times.ttf" />
